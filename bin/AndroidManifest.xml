<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.evconsole"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.evconsole.Login"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MaintainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".AddInaccount"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".HuodaoTest"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".GoodsManager"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".GoodsProSet"
            android:label="@string/app_name" >
        </activity>
       
        <activity
            android:name="com.example.business.Business"
            android:label="@string/app_name" >
        </activity>
    </application>
	<!--往sdcard中写入数据的权限 -->
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
	 <!--在sdcard中创建/删除文件的权限 -->
	<uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"></uses-permission>
</manifest>
